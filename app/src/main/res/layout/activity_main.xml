<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/projectsList"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView_Project1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/projectOne"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView_Project2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/projectTwo"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView_Project3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/projectThree"
            android:textSize="24sp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>